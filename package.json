{
    "name": "tk-hafidatulfikri",
    "version": "1.0.0",
    "description": "Website TK HafidatulFikri",
    "main": "index.js",
    "scripts": {
        "build": "tailwindcss -i ./css/input.css -o ./css/output.css",
        "watch": "tailwindcss -i ./css/input.css -o ./css/output.css --watch",
        "dev": "npm run watch",
        "server": "live-server pages/",
        "start": "concurrently \"npm run watch\" \"npm run server\""
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.7",
        "concurrently": "^9.2.1",
        "live-server": "^1.2.2",
        "tailwindcss": "^3.3.3"
    }
}